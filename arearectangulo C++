// arearectangulo.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	float base, altura, area;
	cout << "Ingrese base: ";
	cin >> base;
	cout << "Ingrese altura: ";
	cin >> altura;
	area=base*altura;
	cout << "El area es: "<<area;
	getch();
}

